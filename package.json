{
  "name": "",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "node esbuild-config/server.prod.mjs",
    "start": "node dist/index.js",
    "migration:generate": "npx sequelize-cli migration:generate --name ",
    "migration:apply": "npx sequelize-cli db:migrate",
    "revert:recent:migration": "npx sequelize-cli db:migrate:undo",
    "revert:all:migration": "npx sequelize-cli db:migrate:undo:all",
    "create:render:migration": "npx node-pg-migrate create --migrations-dir=src/migrations/render",
    "render:apply": "npx node-pg-migrate up --no-check-order --migrations-dir=src/migrations/render",
    "superbase:force:apply": "npx node-pg-migrate up --force --migrations-dir=src/migrations/superbase",
    "superbase:apply:single": "npx node-pg-migrate up --migrations-dir=src/migrations/superbase --name",
    "revert:superbase:migration": "npx node-pg-migrate down --migrations-dir=src/migrations/superbase"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "esbuild": "^0.23.0",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "node-pg-migrate": "^7.5.2",
    "nodemailer": "^6.9.14",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-dist": "^5.18.2",
    "swagger-ui-express": "^5.0.1",
    "validator": "^13.12.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.11",
    "@types/nodemailer": "^6.4.15",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.12.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
